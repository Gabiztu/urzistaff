{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "cd web && npm ci || (cd web && npm install)",
  "buildCommand": "cd web && npm run build",
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/cart", "destination": "/cart.html" },
    { "source": "/checkout", "destination": "/checkout.html" }
  ]
}
